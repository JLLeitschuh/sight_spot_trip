{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
    
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}